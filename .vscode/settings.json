{
    "files.autoSave": "afterDelay",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "typescript",
        "javascript",
        "javascriptreact",
        "html",
        "typescriptreact"
    ],
    "editor.formatOnSave": true,
    "editor.minimap.autohide": true,
    "prettier.bracketSameLine": true
}